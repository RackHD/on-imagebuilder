{
    "package": {
        "name": "on-imagebuilder",
        "repo": "binary", 
        "subject": "rackhd"
    },

    "version": {
        "name": "1.0"
    },

    "files":
        [
            {
	        "includePattern": "/tmp/on-imagebuilder/builds/(.*)", "uploadPattern": "builds/$1", 
                "matrixParams": { "override": 1 }
            },
            {
                "includePattern": "/tmp/on-imagebuilder/ipxe/(.*)", "uploadPattern": "ipxe/$1",
                "matrixParams": { "override": 1 }
            },
            {
                "includePattern": "/tmp/on-imagebuilder/syslinux/(.*)", "uploadPattern": "syslinux/$1",
                "matrixParams": { "override": 1 }
            }
        ],
    "publish": true
}
